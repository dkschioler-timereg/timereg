<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="int" not-null="true"></sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp" length="19" not-null="true"></sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="boolean" length="1" not-null="true"></sql-type>
		<sql-type jdbc-type="TINYINT" hibernate-type="boolean" not-null="true"></sql-type>
	</type-mapping>
	<table-filter match-name=".*" exclude="true" match-schema="timereg"></table-filter>
</hibernate-reverse-engineering>