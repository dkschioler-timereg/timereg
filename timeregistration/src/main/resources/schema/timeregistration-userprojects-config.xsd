   <xs:complexType name="ProjectType">
      <xs:sequence>

         <xs:element name="tasks" type="TaskSetType" minOccurs="1" maxOccurs="1" />
      </xs:sequence>
      <xs:attribute name="name" type="xs:string" use="required" />
      <xs:attribute name="client"  use="required" >
       <xs:simpleType>
            <xs:restriction base="xs:string">
               <xs:enumeration value="tryg.dk" />
               <xs:enumeration value="nordea.com" />
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:complexType>

   <xs:complexType name="TaskSetType">
      <xs:sequence>
         <xs:element name="task" type="TaskType" minOccurs="1" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="TaskType">
      <xs:sequence>
         <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1" />
      </xs:sequence>
      <xs:attribute name="acn-code" type="xs:string" use="required" />
      <xs:attribute name="client-code" type="xs:string" use="required" />
   </xs:complexType>

